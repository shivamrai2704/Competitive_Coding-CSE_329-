problem link-https://www.hackerrank.com/challenges/functional-programming-warmups-in-recursion---gcd/problem
import java.io.*;
import java.util.*;

public class Solution {
    public static int gcd(int a,int b)
     {
        if (a==0)
        return b;
        if (b==0)
        return a;
        else
        return gcd(b%a,a);
     }
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int num1=sc.nextInt();
        int num2=sc.nextInt();
        System.out.println(gcd(num1,num2));
           
    }
}
